{
  "name": "piratesvscode",
  "displayName": "PiratesVSCode",
  "description": "",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:c"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.translaterussian",
        "title": "CL русский"
      },
      {
        "command": "extension.translateenglish",
        "title": "CL english"
      },
      {
        "command": "extension.translatefrench",
        "title": "CL french"
      },
      {
        "command": "extension.translategerman",
        "title": "CL german"
      },
      {
        "command": "extension.translatespanish",
        "title": "CL spanish"
      },
      {
        "command": "extension.translatepolish",
        "title": "CL polish"
      },
      {
        "command": "extension.translateitalian",
        "title": "CL italian"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/ini": "^4.1.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.101.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^3.5.0",
    "esbuild": "^0.25.5",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "vsce": "^2.15.0"
  },
  "dependencies": {
    "ini": "^5.0.0"
  }
}
