{
	"Log": {
		"scope": "",
		"prefix": "log",
		"body": [
			"Log_Info(\"$1\");$2"
		]
	},

	"CommentBlock": {
		"prefix": "block",
		"body": "/**\n${TM_SELECTED_TEXT}  ${1}\n**/${0}",
		"description": "Comment - Block comment",
		"scope": ""
	},

	"Dump": {
		"scope": "",
		"prefix": "dump",
		"body": [
			"DumpAttributes($1);$2"
		]
	},

	"Trace": {
		"scope": "",
		"prefix": "trace",
		"body": [
			"trace(\"$1\");$2"
		]
	},

	"For": {
		"scope": "",
		"prefix": "for",
		"body": ["for (i=$1; i<$2;i++)\n{", "  $0", "}"]
	},

	"CheckAttribute": {
		"scope": "",
		"prefix": "checkat",
		"body": ["CheckAttribute(&$1, $2)$0"]
	},

	"DeleteAttribute": {
		"scope": "",
		"prefix": "delat",
		"body": ["DeleteAttribute(&$1, $2);$0"]
	},

	"IfCheckAttribute": {
		"scope": "",
		"prefix": "ifcheckat",
		"body": ["if (${1:checkat}) $0"]
	},

	"Void": {
		"scope": "",
		"prefix": "voidf",
		"body": ["void $1($2)\n{", "  $0", "}"]
	},

	"Int": {
		"scope": "",
		"prefix": "intf",
		"body": ["int $1($2)\n{", "  $0", "}"]
	},

	"String": {
		"scope": "",
		"prefix": "strf",
		"body": ["string $1($2)\n{", "  $0", "}"]
	}
}